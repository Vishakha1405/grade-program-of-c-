#include<iostream>
using namespace std;
int main()
{
    int marks;
    cout<<("enter the marks of student\n");
    cin>>marks;
    if(marks>=85)
    cout<<("\n\n Your Grade : A");

   else if(marks>=70)
     cout<<("\n\n Your Grade : B");

     else if(marks>=55)
       cout<<("\n\n Your Grade : C");

        else if(marks>=40)
          cout<<("\n\n Your Grade :D ");

   else
     cout<<("\n\n You Grade :F");
     return 0;
   }
